<h1>Projects#new</h1>
<h2>プロジェクト新規作成</h2>

<%= form_with(model: @project, local: true) do |f| %>
  <div>
    <%= f.label :title, "工事件名" %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :site_name, "現場名" %>
    <%= f.text_field :site_name %>
  </div>
  <div>
    <%= f.label :start_date, "開始日" %>
    <%= f.date_field :start_date %>
  </div>
  <div>
    <%= f.label :end_date, "終了日" %>
    <%= f.date_field :end_date %>
  </div>
  <div>
    <%= f.submit "登録" %>
  </div>
<% end %>
